{
  "terminal.integrated.env.osx": {
    "PATH": "${workspaceFolder}/node_modules/.bin"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.rebaseWhenSync": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.validate": ["javascript", "typescript", "javascriptreact", "typescriptreact"],
  "eslint.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.format.enable": true,
  "npm.packageManager": "npm",
  "search-node-modules.path": "node_modules",
  "javascript.format.enable": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "cSpell.words": [
    "Anson",
    "CDPATH",
    "cweijan",
    "dbaeumer",
    "dearmor",
    "debsig",
    "devcontainers",
    "dockerenv",
    "dpkg",
    "DYLD",
    "eamodio",
    "elif",
    "errorlens",
    "esbenp",
    "esbuild",
    "firstname",
    "gpgsign",
    "keyrings",
    "kohler",
    "lastname",
    "libasound",
    "libatk",
    "libatspi",
    "libavif",
    "libcups",
    "libdrm",
    "libegl",
    "libenchant",
    "libepoxy",
    "libevent",
    "libflite",
    "libgbm",
    "libgdk",
    "libgles",
    "libgraphene",
    "libgstreamer",
    "libgudev",
    "libharfbuzz",
    "libhyphen",
    "libmanette",
    "libnotify",
    "libnspr",
    "libnss",
    "libopus",
    "libsecret",
    "libvpx",
    "libwebp",
    "libwebpdemux",
    "libwoff",
    "libx",
    "libxcomposite",
    "libxdamage",
    "libxfixes",
    "libxkbcommon",
    "libxrandr",
    "libxtst",
    "mikestead",
    "MSYS",
    "nistp",
    "noninteractive",
    "nslookup",
    "onepassword",
    "outform",
    "pixbuf",
    "pkcs",
    "PKCS",
    "pkey",
    "pubout",
    "PYTHONHOME",
    "PYTHONPATH",
    "Shellcheck",
    "signingkey",
    "SSHKEY",
    "Subsetting",
    "subshell",
    "subshells",
    "Terroir",
    "urandom",
    "usernamehw",
    "venv",
    "voyek",
    "wcag",
    "WEBP",
    "yoavbls",
    "yzhang"
  ],
  "editor.fontFamily": "'Intel One Mono', 'Roboto Mono for Powerline', monospace",
  "editor.fontSize": 13,
  "cSpell.ignoreWords": ["foobarwoo", "sannysoft", "testuser", "sess"]
}
